@model System.Collections.Generic.List<MvcUI.Controllers.TempClass>?

@{
    ViewBag.Title = "title";
    Layout = "/Areas/Admin/Views/Shared/AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Full Name</th>
        <th>Delete</th>
        <th>Edit</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td><a href="" class="btn btn-outline-danger">Delete</a></td>
            <td><a href="" class="btn btn-outline-warning">Edit</a></td>
        </tr>
    }
</table>
<a asp-controller="Employee" asp-action="AddEmployee" class="btn btn-outline-primary">New Employee</a>
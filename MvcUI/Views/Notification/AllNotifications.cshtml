@model System.Collections.Generic.List<Entities.Concrete.Notification>

@{
    ViewBag.Title = "title";
    Layout = "WriterLayout";
}

<h1>Notifications</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Notification</th>
        <th>Notification Date</th>
    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.NotificationId</td>
            <td>@item.Details</td>
            <td>@item.Date.ToShortDateString()</td>
        </tr>
    }
</table>